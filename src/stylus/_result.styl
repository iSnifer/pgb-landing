.result
    text-align center
    padding-top 60px
    margin-bottom 10px

    &__title
        font-size 32px
        margin 0 0 20px

    &__item
        display inline-block
        margin 0 25px 5px

    &__icon
        display inline-block
        width 25px
        height 19px
        bg(result, icon)

    &__text
        font-size 18px
        display block
